#include <stdio.h>
#include "stack.h"

int
main(int argc, char** argv)
{
	int* m;
	stack s;

	s = STACK_EMPTY;

	printf("Staring Push");
	for(i = 0; i < 5; i++){
		m = malloc(sizeof(int));
		*m = i;
		printf("push %d\n", m);
		stackPush(&s,&i);
	}


	while(!stackEmpty(&s)){
		printf("pop gets %d\n", *((int *)stackPop(&s)));
	}
}
