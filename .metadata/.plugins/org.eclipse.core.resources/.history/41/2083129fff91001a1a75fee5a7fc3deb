#include <stdio.h>
#include "stack.h"

int
main(int argc, char** argv)
{
	int i;
	stack s;

	s = STACK_EMPTY;

	print("Staring Push");
	for(i = 0; i < 5; i++){
		printf("push %d\n", i);
		stackPush(&s,&i);
		stackPrint(&s);
	}

	while(!stackEmpty(&s)){
		printf("pop gets %d\n", (int)stackPop(&s));
		stackPrint(&s);
	}
}
