#include <stdio.h>
#include <stdlib.h>
#include "stack.h"

typedef dt float;

int
main(int argc, char** argv)
{
	dt* m;
	int i;
	stack s;

	s = STACK_EMPTY;

	for(i = 0; i < 5; i++){
		m = malloc(sizeof(int));
		*m = i;
		printf("push %d\n", *m);
		stackPush(&s,m);
	}


	while(!stackEmpty(&s)){
		m = (dt *)stackPop(&s);
		printf("pop gets %d\n",*m);
		free(m);
	}
}
