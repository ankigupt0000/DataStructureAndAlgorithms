#include <stdio.h>
#include <stdlib.h>
#include "stack.h"

int
main(int argc, char** argv)
{
	float* m;
	int i;
	stack s;

	s = STACK_EMPTY;

	for(i = 0; i < 5; i++){
		m = malloc(sizeof(int));
		*m = i;
		printf("push %d\n", *m);
		stackPush(&s,m);
	}


	while(!stackEmpty(&s)){
		m = (int *)stackPop(&s);
		printf("pop gets %d\n",*m);
		free(m);
	}
}
